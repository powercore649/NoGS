{
    "name": "NoGS",
    "version": "1.0.0",
    "description": "Ensures you are connected to a VPN before going to particular sites.",
    "manifest_version": 3,
    "author": "Andy Villasmil",
    "permissions": ["storage", "tabs", "webRequest", "*://*/*"],
    "background": {
        "service_worker": "js/background.js"
    },
    "action":{
        "default_popup": "index.html",
        "default_title": "NoGS"
    }
}