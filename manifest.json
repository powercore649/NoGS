{
    "name": "NoGS VPN Checker",
    "version": "1.3.3",
    "description": "Chrome Extension that ensures you are connected to a VPN before going to blocked sites.",
    "manifest_version": 3,
    "author": "Andy Villasmil",
    "permissions": ["storage", "tabs"],
    "background": {
        "service_worker": "js/background.js"
    },
    "action":{
        "default_popup": "html/index.html",
        "default_title": "NoGS VPN Checker"
    },
    "icons": { 
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png" 
    }
    
}